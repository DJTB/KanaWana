{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": "src/",
    "includePattern": "(index|core|utils).js$",
    "excludePattern": "(node_modules/|docs|tests)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  },
  "opts": {
    "template": "node_modules/docdash/",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true
  },
  "docdash": {
    "sort": false
  }
}
